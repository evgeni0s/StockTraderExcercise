<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:v="clr-namespace:StockTraderExcercise.Views"
    xmlns:m="clr-namespace:StockTraderExcercise.Models"
    xmlns:vm="clr-namespace:StockTraderExcercise.ViewModels">
    <DataTemplate DataType="{x:Type vm:CreateStockViewModel}">
        <v:CreateStockView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StockProprtiesViewModel}">
        <v:StocksPropertiesView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:StocksViewModel}">
        <v:StocksView />
    </DataTemplate>
    <DataTemplate DataType="{x:Type m:StockProperties}">
        <v:StockPropertiesDetails />
    </DataTemplate>
</ResourceDictionary>